{
	"name": "botbyte",
	"description": "An abstraction layer for creating Byteball bot",
	"version": "0.0.0",
	"author": "Fahmi Akbar Wildana @DrSensor",
	"bin": {
		"botbyte": "dist/index.js"
	},
	"bugs": "https://github.com/DrSensor/bot-byte/issues",
	"dependencies": {
		"@oclif/command": "^1.5.6",
		"@oclif/config": "^1.9.0",
		"@oclif/plugin-help": "^2.1.4",
		"byteballcore": "git+https://github.com/byteball/byteballcore.git",
		"headless-byteball": "git+https://github.com/byteball/headless-byteball.git",
		"tslib": "^1.9.3"
	},
	"devDependencies": {
		"@babel/core": "^7.2.0",
		"@babel/plugin-proposal-class-properties": "^7.2.1",
		"@babel/plugin-proposal-export-namespace-from": "^7.2.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.2.0",
		"@babel/preset-env": "^7.2.0",
		"@babel/preset-typescript": "^7.1.0",
		"@commitlint/cli": "^7.2.1",
		"@commitlint/config-conventional": "^7.1.2",
		"@oclif/tslint": "^3.1.1",
		"@types/chai": "^4.1.7",
		"@types/glob": "^7.1.1",
		"@types/jest": "^23.3.10",
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.12.15",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^23.6.0",
		"conventional-github-releaser": "^3.1.2",
		"glob": "^7.1.3",
		"husky": "^1.2.1",
		"jest": "^23.6.0",
		"jest-silent-reporter": "^0.1.1",
		"lint-staged": "^8.1.0",
		"prettier": "^1.15.3",
		"rollup": "^0.67.4",
		"rollup-plugin-alias": "^1.5.1",
		"rollup-plugin-run": "^1.1.0",
		"rollup-plugin-auto-external": "^2.0.0",
		"rollup-plugin-babel": "^4.1.0",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-node-resolve": "^4.0.0",
		"rollup-plugin-prettier": "^0.5.0",
		"rollup-plugin-typescript2": "^0.18.1",
		"shx": "^0.3.2",
		"standard-version": "^4.4.0",
		"ts-jest": "^23.10.5",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"typescript": "^3.2.2"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"/bin",
		"/lib"
	],
	"homepage": "https://github.com/DrSensor/bot-byte",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"oclif": {
		"bin": "dist/main.js"
	},
	"repository": "DrSensor/bot-byte",
	"lint-staged": {
		"linters": {
			"*.ts": [
				"tslint --fix -c tslint.json -p tsconfig.json"
			]
		},
		"ignore": [
			"examples/**"
		]
	},
	"standard-version": {
		"scripts": {
			"postbump": "prettier package*.json --write",
			"precommit": "git add CHANGELOG.md package*.json"
		}
	},
	"scripts": {
		"posttest": "tslint -p . -t stylish",
		"prepack": "rm -rf lib && tsc --build",
		"prebuild": "shx rm -r dist || true",
		"build": "rollup -c",
		"prestart": "npm run prebuild",
		"start": "rollup -c -w || true",
		"lint": "(tsc --pretty -p . --noemit || true) && tslint -p . --force",
		"lint:fix": "tslint -p . --fix --force",
		"lint:commits": "commitlint --to=$(git rev-parse HEAD)",
		"release": "standard-version --skip.commit --skip.tag",
		"release:as": "standard-version --skip.commit --skip.tag --release-as",
		"release:ok": "standard-version --sign --commit-all --skip.bump --skip.changelog",
		"release:validate": "commitlint --from=$(git describe --tags --abbrev=0) --to=$(git rev-parse HEAD)",
		"ci:lint": "tsc --pretty -p . --noemit && tslint -p .",
		"ci:test": "jest --runInBand --silent --ci",
		"ci:coverage": "jest --collectCoverageFrom='src/**/*.ts' --coverage --runInBand --silent --ci",
		"ci:release": "conventional-github-releaser -p angular",
		"ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
		"pretest": "npm run build",
		"test": "jest --notify",
		"test:watch": "npm test -- --watch || true",
		"test:coverage": "jest --collectCoverageFrom='src/**/*.ts' --coverage",
		"prepublishOnly": "npm run build"
	},
	"types": "lib/index.d.ts"
}
